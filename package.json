{
  "name": "flight-logger",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "concurrently \"vite-node app/server.ts\" \"npm run schedule-data-updates\"",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --watch app --watch routes --exec \"vite-node app/server.ts\"",
    "dev:client": "vite",
    "generate": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset",
    "seed": "vite-node app/commands/seedDatabase.ts",
    "prebuild": "rm -rf prisma/generated dist && npm run generate && npx tsc --noEmit && npx eslint . && npm run format:check",
    "build": "NODE_ENV=\"production\" vite build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage": "vitest --coverage",
    "preview": "vite preview",
    "format": "prettier --config ./.prettierrc -w app resources vite.config.ts",
    "format:check": "prettier --config ./.prettierrc --check app resources vite.config.ts",
    "schedule-data-updates": "vite-node app/commands/scheduleDataUpdates.ts",
    "test-data-update": "vite-node app/commands/testDataUpdate.ts"
  },
